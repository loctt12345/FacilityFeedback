@page
@model FacilityFeedback.RazorPage.Pages.FloorPage.CreateModel

@{
    ViewData["Title"] = "Create";
}



<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-sm-12 col-xl-6">
            <div class="bg-light rounded h-100 p-4">
                <form>
                    <div class="mb-3">
                        @Html.LabelFor(m => m.Floor.Code, new {@class = "form-label"})
                        @Html.TextBoxFor(m => m.Floor.Code, new {@class = "form-control"})
                    </div>
                    <div class="mb-3">
                        @Html.LabelFor(m => m.Floor.Status, new {@class = "form-label"})
                        @Html.TextBoxFor(m => m.Floor.Status, new {@class = "form-control"})
                    </div>
                    <div class="mb-3">
                        @Html.LabelFor(m => m.Floor.Id, new {@class = "form-label"})
                        @Html.TextBoxFor(m => m.Floor.Id, new {@class = "form-control"})
                    </div>

                    <button type="submit" class="btn btn-primary">Create</button>
                </form>

                <div>
                    <a asp-page="Index">Back to List</a>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
